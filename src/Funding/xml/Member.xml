<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" 
"http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="Funding.Mapper.MemberMapper">		
	<cache />
	
	<select id="listMember"  resultType="Member">
		select * from member
	</select>
	
	<select id="infoMember" parameterType="int" resultType="Member">
	select * from member where m_num=#{m_num}<!-- 마이페이지 -->
	</select>
	
	
</mapper>