<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" 
"http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="Funding.Mapper.ContentsMapper">		<!-- 이렇게 되면 같은 경로에 BoardMapper 인터페이스가 필요하다. -->
	<cache />
	<select id="DetailContents" parameterType="int" resultType="Contents">
		select * from Contents
		where c_num = #{c_num}
	</select>
	
	<select id="AllListContents" resultType="Contents" > 
	select * from Contents
	 </select>

	<insert id="InsertContents" parameterType="Contents" >
		insert into contents(
			c_num,c_title,c_cate,c_intro,c_text,c_midimg,c_topimg,c_dead,c_goal,c_thimg
		)values(
			#{c_num},#{c_title},#{c_cate},#{c_intro},#{c_text},#{c_midimg},#{c_topimg},#{c_dead},#{c_goal},#{c_thimg}
		)
		
	</insert>	

</mapper>